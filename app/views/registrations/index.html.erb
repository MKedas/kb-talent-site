<h1>Listing registrations</h1>
<div class="well table-responsive container text-center" style="color:black;">
<table class="table">
  <thead>
    <tr>
      <th>First</th>
      <th>Last</th>
      <th>DOB</th>
      <th>Email</th>
      <th>Phone #</th>
      <th>Video</th>
      <th>First mp3</th>
      <th>Second mp3</th>
      <th>Agreed to Terms</th>
      <th>Payment Confirmed</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @registrations.each do |registration| %>
      <tr>
        <td><%= registration.first_name %></td>
        <td><%= registration.last_name %></td>
        <td><%= registration.date_of_birth %></td>
        <td><%= registration.email %></td>
        <td><%= registration.phone_number %></td>
        <td><%= link_to 'Video', registration.video %></td>
        <td><%= link_to 'Song 1', registration.first_mp3 %></td>
        <td><%= link_to 'Song 2', registration.second_mp3 %></td>
        <td><%= registration.agreed_to_terms %></td>
        <td><%= registration.payment_confirmed %></td>
        <td><%= link_to 'Show', registration , class: 'btn-sm btn-info'%></td>
        <td><%= link_to 'Edit', edit_registration_path(registration) , class: 'btn-sm btn-primary'%></td>
        <td><%= link_to 'Destroy', registration, method: :delete, data: { confirm: 'Are you sure?' } , class: 'btn-sm btn-danger'%></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
